<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="in.koala.mapper.CrawlingMapper">

    <select id="test" resultType="string">
        SELECT title FROM crawling LIMIT 1;
    </select>

    <insert id="addCrawlingData">
        INSERT INTO crawling(title, url, site, created_at)
        VALUES
        <foreach item="crawling" separator=" , " collection="list   ">
            (
             #{crawling.title},
             #{crawling.url},
             #{crawling.site},
             #{crawling.createdAt}
            )
        </foreach>
    </insert>

    <!--<insert id="addCrawlingData" parameterType="in.koala.domain.Crawling">
        INSERT INTO crawling(title, url, site, created_at)
        VALUES (#{title}, #{url}, #{site}, #{createdAt});
    </insert>-->

</mapper>